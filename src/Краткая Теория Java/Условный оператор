        int j = 2;
        if (i != 234) { // Проверки >, <, >=, <=, !=
            // Тут выполняется код при истинности условия
            if (){ // Можно помещать вложенные условные операторы
                //Код вложенного условного оператора
            }
        } else if () { //Дополнительная проверка
            //Выполняется код, если не выполнился if
        } else{
            //Выполнится если не один из if не выполнился
        }

        if (i != 234 && j>2){ // && - и    || - или
            //Код
        }

