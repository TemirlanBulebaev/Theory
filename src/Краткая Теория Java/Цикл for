package com.company;


public class Main {

    public static void main(String[] args) {
        for (int i = 0; i < 10; i++) {
            System.out.println("i = " + i);
        }
        for (int i = 100; i >= 10; i -= 10) { //Можно делать i *= 2 и т.д.
            System.out.println("i = " + i);
        }
    }

}
